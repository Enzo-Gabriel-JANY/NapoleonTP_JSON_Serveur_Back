{
  "name": "json-server-backend",
  "version": "1.0.0",
  "description": "Backend with JSON Server",
  "main": "index.js",
  "scripts": {
    "start": "json-server --watch db.json",
    "dev": "nodemon --watch db.json --exec 'json-server --watch db.json'",
    "back": "json-server --watch db.json --port 5000"
  },
  "dependencies": {
    "json-server": "^0.17.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
